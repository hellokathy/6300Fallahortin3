package edu.gatech.seclass.gradescalc;

public class Student {
	String name;
	int gtID;
	//String email;
	int attendance;
	public Student(String name, String id, GradesDB db){
		this.name = name;
		this.getID = id; 
		db.getStudentByID(id);
		
	};
	
	public Student(String name, int id, int attendance) {
		this.name = name;
		this.gtID = id;
		this.attendance = Integer.valueOf(attendance);
	}

	public String getName() {
		return this.name;
	}

	public String getGtid() {
		return String.valueOf(this.gtID);
	}

	public Object getAttendance() {
		return this.attendance;
	}

}
