	//not required, and poor since it is dependent on testGetString
	@Test
	public void testSetString() {
		MyString testString = new MyString();
		testString.setString("bulbasaur");
		assertEquals("should equal bulbasaur", "bulbasaur", testString.getString());
	}
	
	@Test
	public void testGetString() {
		assertEquals("should still equal bulbasaur", "sbulbasaur", testString.getString());
